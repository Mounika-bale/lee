#include<stdio.h>
#include<string.h>
int main()
{
   char s[100];
   int i,j,n,count=1;
   printf("enter any string\n");
   gets(s);
   n=strlen(s);
   for(i=0;i<n;i++){
    if(s[i]){
      for(j=i+1;j<n;j++){
          if(s[i]==s[j{
              count++;
              s[j]='\0';
              }
            }
         printf("%c - %d\n",s[i],count);
        }
      }
   return 0;   
}      

      
      
